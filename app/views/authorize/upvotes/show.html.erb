<div class="container">
  <div class="row">
    <div class="col-2">
      <%= link_to 'Back to Video', @video_post, class: "btn btn-secondary"%>
    </div>
  </div>
  <div class="row">
    <h5>Users that upvoted this video:</h5>
  </div>
  <div class="row">
    <% @upvotes.find_each do |upvote|%>
      <%= link_to upvote.user.username ,profile_path(upvote.user)%><br>
    <%end%>
  </div>
</div>